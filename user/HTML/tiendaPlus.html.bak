    <script>
        // funcion para activar el menu y desactivrlo
        function cambiarClase() {
            var div = document.getElementById("lista_carrito");
            if (div.classList.contains("active")) {
                div.classList.remove("active");
                 document.body.style.setProperty('--before-z-index', '3');
                 document.body.style.overflowY = 'hidden';
                
            } else {
                div.classList.add("active");
                 document.body.style.setProperty('--before-z-index', '0');
                  document.body.style.overflowY = 'auto';
                 
            }
        }
       
        
        function cambiarClase_panel_busqueda() {
            var div = document.getElementById("panel_busquedas");
            if (div.classList.contains("active")) {
                div.classList.remove("active");
                 document.body.style.setProperty('--before-z-index', '3');
                 document.body.style.overflowY = 'hidden';
                
            } else {
                div.classList.add("active");
                 document.body.style.setProperty('--before-z-index', '0');
                  document.body.style.overflowY = 'auto';
                 
            }
        }

        function cambiarClase_panel_filtros_lateral() {
            var div = document.getElementById("panel_resposivo_filtro");
            if (div.classList.contains("active")) {
                div.classList.remove("active");
                if (window.innerWidth <= 768) {
                    document.body.style.setProperty('--before-z-index', '3');
                    document.body.style.overflowY = 'hidden';
                }
                
                
            } else {
                div.classList.add("active");
                 document.body.style.setProperty('--before-z-index', '0');
                  document.body.style.overflowY = 'auto';
                 
            }
        }
        
         function abrir_inicar_sesion() {
            var div = document.getElementById("id_modal_iniciar_sesion_like");
            if (div.classList.contains("abrir_modal")) {
                div.classList.remove("abrir_modal");
                document.body.style.setProperty('--before-z-index', '3');
                 document.body.style.overflowY = 'hidden';
            } else {
                div.classList.add("abrir_modal");
                 document.body.style.setProperty('--before-z-index', '0');
                  document.body.style.overflowY = 'auto';
            }
        }
        function cambiarClaseFlitro() {
            var div = document.getElementById("carcasa");
            var filtro = document.getElementById("flitros");
            
           
            
            if (div.classList.contains("active")) {
                div.classList.remove("active");
                setTimeout(() => {
                    filtro.classList.remove("active");
                }, 200);
                
                
            } else {
                    filtro.classList.add("active"); 
                setTimeout(() => {
                    div.classList.add("active");
                }, 100);
            }
        }
        function validaNumericos(event) {
            if(event.charCode >= 48 && event.charCode <= 57){
              return true;
             }
             return false;        
        }
        function abrir_producto(){
            window.location.href = "Producto.html";
        }
        function cambiar_iniciar_sesion(c){
            if (document.getElementById('perfil').innerHTML=='INGRESAR'
            || document.getElementById('perfil-nav').innerHTML=='INGRESAR'){
                window.location.href = "\login?like="+c;
            }
        }
        
        var sv1 = document.getElementById("svg_1_puntos");
        var sv2 = document.getElementById("svg_2_puntos");
        var sv3 = document.getElementById("svg_3_puntos");
        var sv4 = document.getElementById("svg_4_puntos");
         function cambiarGridCartas_svg_1_puntos(){
             var sv1 = document.getElementById("svg_1_puntos");
        var sv2 = document.getElementById("svg_2_puntos");
        var sv3 = document.getElementById("svg_3_puntos");
        var sv4 = document.getElementById("svg_4_puntos");
            const  div = document.getElementById("gridContainer");
            div.classList.remove("dos_puntos");
            div.classList.remove("tres_puntos");
            div.classList.remove("cuatro_puntos");
             div.classList.add("un_punto");
             
             sv1.style.fill ="#f15d2a";
             sv2.style.fill ="#F09F82";
             sv3.style.fill ="#F09F82";
             sv4.style.fill ="#F09F82";
         }
         function cambiarGridCartas_svg_2_puntos(){
             var sv1 = document.getElementById("svg_1_puntos");
        var sv2 = document.getElementById("svg_2_puntos");
        var sv3 = document.getElementById("svg_3_puntos");
        var sv4 = document.getElementById("svg_4_puntos");
             var div = document.getElementById("gridContainer");
              div.classList.remove("un_punto");
            div.classList.remove("tres_puntos");
            div.classList.remove("cuatro_puntos");
             div.classList.add("dos_puntos");
             
             sv1.style.fill ="#F09F82";
             sv2.style.fill ="#f15d2a";
             sv3.style.fill ="#F09F82";
             sv4.style.fill ="#F09F82";
         }
       
         function cambiarGridCartas_svg_3_puntos(){
            var sv1 = document.getElementById("svg_1_puntos");
            var sv2 = document.getElementById("svg_2_puntos");
            var sv3 = document.getElementById("svg_3_puntos");
            var sv4 = document.getElementById("svg_4_puntos");
             var div = document.getElementById("gridContainer");
          div.classList.remove("dos_puntos");
            div.classList.remove("un_punto");
            div.classList.remove("cuatro_puntos");
             div.classList.add("tres_puntos");
             
             sv1.style.fill ="#F09F82";
             sv2.style.fill ="#F09F82";
             sv3.style.fill ="#f15d2a";
             sv4.style.fill ="#F09F82";
         }
       
         function cambiarGridCartas_svg_4_puntos(){
             var sv1 = document.getElementById("svg_1_puntos");
        var sv2 = document.getElementById("svg_2_puntos");
        var sv3 = document.getElementById("svg_3_puntos");
        var sv4 = document.getElementById("svg_4_puntos");
             var div = document.getElementById("gridContainer");
             
              div.classList.remove("dos_puntos");
            div.classList.remove("tres_puntos");
            div.classList.remove("un_punto");
             div.classList.add("cuatro_puntos");
             
             sv1.style.fill ="#F09F82";
             sv2.style.fill ="#F09F82";
             sv3.style.fill ="#F09F82";
             sv4.style.fill ="#f15d2a";
         }
       
    </script>
