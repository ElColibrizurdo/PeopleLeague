<script>
  const perfil = document.getElementById('perfil');
         const modal_perfil = document.getElementById('modal_perfil');
         let hideTimeout;
         
         perfil.addEventListener('click', () => {
             
             
             if (localStorage.getItem('sesion')) {
                clearTimeout(hideTimeout);  // Limpiar cualquier timeout anterior
                modal_perfil.classList.remove("abrir_modal");
                setHideTimeout();
             } else {

                window.href.location = '/login'
             }

             
         });
         
         modal_perfil.addEventListener('click', () => {
             clearTimeout(hideTimeout);  // Limpiar cualquier timeout anterior
             setHideTimeout();
         });
          modal_perfil.addEventListener('mouseover', () => {
             clearTimeout(hideTimeout);  // Limpiar cualquier timeout anterior
         });

         modal_perfil.addEventListener('mouseleave', () => {
             setHideTimeout();
         });
         document.addEventListener('click', (event) => {
             if (!modal_perfil.contains(event.target) && event.target !== perfil) {
                 setHideTimeout();
             }
         });

         function setHideTimeout() {
             clearTimeout(hideTimeout);  // Limpiar cualquier timeout anterior
             hideTimeout = setTimeout(() => {
                 modal_perfil.classList.add("abrir_modal");
             }, 1000);  // El div se ocultará después de 3 segundos
         }
 
  const rangeInput = document.querySelectorAll(".range-input input"),
priceInput = document.querySelectorAll(".price-input input"),
range = document.querySelector(".slider .progress");
let priceGap = 1000;
priceInput.forEach(input =>{
input.addEventListener("input", e =>{
 let minPrice = parseInt(priceInput[0].value),
 maxPrice = parseInt(priceInput[1].value);
 
 if((maxPrice - minPrice >= priceGap) && maxPrice <= rangeInput[1].max){
     if(e.target.className === "input-min"){
         rangeInput[0].value = minPrice;
         range.style.left = ((minPrice / rangeInput[0].max) * 100) + "%";
     }else{
         rangeInput[1].value = maxPrice;
         range.style.right = 100 - (maxPrice / rangeInput[1].max) * 100 + "%";
     }
 }
});
});
rangeInput.forEach(input =>{
input.addEventListener("input", e =>{
 let minVal = parseInt(rangeInput[0].value),
 maxVal = parseInt(rangeInput[1].value);
 if((maxVal - minVal) < priceGap){
     if(e.target.className === "range-min"){
         rangeInput[0].value = maxVal - priceGap;
     }else{
         rangeInput[1].value = minVal + priceGap;
     }
 }else{
     priceInput[0].value = minVal;
     priceInput[1].value = maxVal;
     range.style.left = ((minVal / rangeInput[0].max) * 100) + "%";
     range.style.right = 100 - (maxVal / rangeInput[1].max) * 100 + "%";
 }
});
});

const paragraphs = document.querySelectorAll('.titulos_filtos');

// Recorre cada <p> y añade el evento click
paragraphs.forEach(p => {
  p.addEventListener('click', function() {
    // Encuentra el <svg> dentro del <p> clickeado
    const svg = this.querySelector('svg');

    // Alterna la clase "active" en el <svg>
    if (svg) {
      svg.classList.toggle('active');
    }
  });
});

</script>

