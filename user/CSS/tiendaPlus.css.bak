                    .buscador{
                        cursor: pointer;
                        width: 135px;
                        padding: 10px 0px 10px 40px;
                        border-radius: 9999px;
                        border: solid rgba(0, 0, 0, 0);
                        transition: all .2s ease-in-out;
                        outline: none;
                        opacity: 1;
                        font-size: 17px;
                        font-style: normal;
                        font-weight: 900;
                        line-height: normal;
                        color: #004789;
                        background-color: rgba(0, 0, 0, 0); 
                      }
                      .buscador::placeholder {
                        color: #004789; /* Cambia este color por el que desees */
                        opacity: 1; /* Para asegurar que el color no se vea transparente */
                      }
                      /*.container-buscador {
                        display: none;
                        }*/
                      .container-buscador {
                        position: relative;
                        background-color:whitesmoke;
                        z-index: 0;
                      }
                      .container-buscador svg {
                        position: absolute;
                        top: 50%;
                        left: 10px;
                        transform: translate(0, -50%);
                      }

                      .buscador:focus {
                       border: solid 3px #f15d2a;
                       opacity: 1;
                        width: 450px;
                         color: #1C1C1C;
                      }
                      
                      
                    .btn_carrito {
                        display: inline-flex; 
                        align-items: center; 
                        justify-content: center;
                        padding: 10px 18px;
                        border: solid #f15d2a;
                        border-radius: 10px;
                        color: #f15d2a; 
                        z-index: 1;
                        gap: 8px;
                        background: #FFFFFF;
                        position: relative;
                        transition: all 200ms;
                        overflow: hidden;
                       }

                       .btn_carrito::before {
                        content: "";
                        position: absolute;
                        top: 0;
                        left: 0;
                        height: 100%;
                        width: 0;
                        border-radius: 0 ;
                        background-color: #f15d2a;
                        z-index: -1;
                        transition: all 250ms;
                       }

                       .btn_carrito:hover {
                        color: #FFFFFF;
                       }
                       
                       .svg_carrito{
                           width:24px; 
                           height:24px;
                           fill:#f15d2a;
                           
                       }
                       .btn_carrito:hover .svg_carrito{
                             fill:#FFFFFF;
                       }

                      .btn_carrito:hover::before {
                        width: 100%;
                       }
